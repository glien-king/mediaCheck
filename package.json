{
  "name": "mediaCheck",
  "version": "0.5.0",
  "homepage": "http://github.com/sparkbox/mediaCheck",
  "scripts": {
    "test": "snyk test && karma start --single-run",
    "start": "npm-watch",
    "build": "npm test && node prependBanner ./js/mediaCheck.js",
    "dist": "npm run build && npm run uglify && node prependBanner ./js/mediaCheck-min.js",
    "uglify": "uglifyjs ./js/mediaCheck.js > ./js/mediaCheck-min.js",
    "postversion": "npm run dist"
  },
  "watch": {
    "build": {
      "patterns": [
        "coffee"
      ],
      "extensions": "coffee"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.4",
    "coffee-script": "^1.11.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "npm-watch": "^0.1.6",
    "phantomjs-prebuilt": "^2.1.13",
    "prepend-file": "^1.3.0",
    "snyk": "^1.19.1",
    "uglify-js": "^2.7.3"
  }
}
